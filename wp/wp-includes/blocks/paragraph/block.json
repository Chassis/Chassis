{
	"apiVersion": 2,
	"name": "core/paragraph",
	"category": "text",
	"attributes": {
		"align": {
			"type": "string"
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p",
			"default": ""
		},
		"dropCap": {
			"type": "boolean",
			"default": false
		},
		"placeholder": {
			"type": "string"
		},
		"direction": {
			"type": "string",
			"enum": [
				"ltr",
				"rtl"
			]
		}
	},
	"supports": {
		"anchor": true,
		"className": false,
		"color": {
			"link": true
		},
		"fontSize": true,
		"lineHeight": true,
		"__experimentalSelector": "p",
		"__unstablePasteTextInline": true
	}
}
